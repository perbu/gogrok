{{ define "localModules" }}
    <div id="module">
        <h2>Local Modules</h2>
        <ul>
            {{ range .Modules }}
                <li>
                    <a href="#" hx-get="/module/{{ .Path }}" hx-target="#module">{{ .Path }}</a> - Packages: {{ .PackagesCount }}, Files: {{ .FilesCount }}, Lines of Code: {{ .LinesOfCode }}
                </li>
            {{ end }}
        </ul>
    </div>
    <div class="container" id="package">

    </div>
{{ end }}
